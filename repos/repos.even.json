[
  {
    "kernelSource": {
      "name": "Even-Kernel",
      "repo": "https://github.com/liquidprjkt/liquid_kernel_realme_even.git",
      "branch": "OSS-RUI2",
      "device": "even",
      "defconfig": "even_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        
      "repo": "https://github.com/EternalX-project/aarch64-linux-gnu",
        "branch": "amd64",
        "name": "aarch64",
        "binPath": ["bin"]
      },
      {
        "repo": "https://github.com/EternalX-project/arm-linux-gnueabi",
        "branch": "amd64",
        "name": "arm32",
        "binPath": ["bin"]
      },
      {
        "repo": "https://github.com/ArrowOS-Devices/android_prebuilts_clang_host_linux-x86_clang-r416183b1",
        "branch": "arrow-11.0",
        "name": "clang",
        "binPath": ["bin"]
      }
  
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "CROSS_COMPILE": "aarch64/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm32/bin/arm-linux-gnueabi-",
        "LD": "clang/bin/ld.lld",
        "AR": "clang/bin/llvm-ar",
        "NM": "clang/bin/llvm-nm",
        "OBJCOPY": "clang/bin/llvm-objcopy",
        "OBJDUMP": "clang/bin/llvm-objdump",
        "READELF": "clang/bin/llvm-readelf",
        "OBJSIZE": "clang/bin/llvm-size",
        "STRIP": "clang/bin/llvm-strip",
        "LDGOLD": "clang/bin/aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "clang/bin/llvm-ar",
        "LLVM_DIS": "clang/bin/llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/TheUnknownName/AnyKernel3/",
      "branch": "even"
    }
  }
]
