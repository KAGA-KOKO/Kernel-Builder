[
  {
    "kernelSource": {
      "name": "Even-Kernel",
      "repo": "https://github.com/Rem01Gaming/liquid_kernel_realme_even.git",
      "branch": "OSS-RUI2",
      "device": "even",
      "defconfig": "even_defconfig"
    },
    "withKernelSU": true,
    "clang": [
      {
        
      "repo": "https://github.com/EternalX-project/aarch64-linux-gnu.git",
        "branch": "amd64",
        "name": "aarch64",
        "binPath": ["bin"]
      }]

    "aarch64": [
      {
        
      "repo": "https://github.com/EternalX-project/aarch64-linux-gnu.git",
        "branch": "amd64",
        "name": "aarch64",
        "binPath": ["bin"]
      }]

    "gcc": [
      {
        
      "repo": "https://github.com/EternalX-project/aarch64-linux-gnu.git",
        "branch": "amd64",
        "name": "aarch64",
        "binPath": ["bin"]
      }]
      
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "CROSS_COMPILE": "aarch64/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "clang/bin/arm-linux-gnueabi-",
        "LD": "clang/bin/ld.lld",
        "AR": "clang/bin/llvm-ar",
        "NM": "clang/bin/llvm-nm",
        "OBJCOPY": "clang/bin/llvm-objcopy",
        "OBJDUMP": "clang/bin/llvm-objdump",
        "READELF": "clang/bin/llvm-readelf",
        "OBJSIZE": "clang/bin/llvm-size",
        "STRIP": "clang/bin/llvm-strip",
        "LDGOLD": "clang/bin/aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "clang/bin/llvm-ar",
        "LLVM_DIS": "clang/bin/llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/TheUnknownName/AnyKernel3/",
      "branch": "even"
    }
  }
]
