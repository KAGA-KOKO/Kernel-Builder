[
  {
    "kernelSource": {
      "name": "M12-Kernel",
      "repo": "https://github.com/SpacingBat3/android_kernel_samsung-m127f_a13",
      "branch": "4.19.111-samsung",
      "device": "m12",
      "defconfig": "exynos850-m12nsxx-spacingbat3_defconfig"
    },
    "withKernelSU": false,
    "toolchains": [
      {
        "repo": "https://github.com/SpacingBat3/toolchains-for-exynos-850",
        "branch": "main",
        "name": "./toolchains",
        "binPath": ["./bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "./toolchains/android_prebuilts_clang_host_linux-x86_clang-5484270-9.0/bin/",
      "externalCommand": {
        "CROSS_COMPILE": "./toolchains/aarch64-linux-android-4.9/bin/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "./toolchains/proton-clang-13-clang/bin/aarch64-linux-gnu-",
        "LD": "toolchains-for-exynos-850/bin/ld.lld",
        "AR": "./toolchains/bin/llvm-ar",
        "NM": "./toolchainsbin/llvm-nm",
        "OBJCOPY": "./toolchains/bin/llvm-objcopy",
        "OBJDUMP": "./toolchains/bin/llvm-objdump",
        "READELF": "./toolchains/bin/llvm-readelf",
        "OBJSIZE": "./toolchains/bin/llvm-size",
        "STRIP": "./toolchains/bin/llvm-strip",
        "LDGOLD": "./toolchains/bin/aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "./toolchains/bin/llvm-ar",
        "LLVM_DIS": "./toolchains/bin/llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": false,
      "repo": "https://github.com/TheUnknownName/AnyKernel3/",
      "branch": "even"
    }
  }
]
